<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension id="osgifx" point="org.eclipse.core.runtime.applications">
      <application cardinality="*" thread="any" visible="true">
         <run class="in.bytehue.osgifx.console.application.ConsoleFxApplication" />
      </application>
   </extension>
	<extension id="product" point="org.eclipse.core.runtime.products">
		<product name="OSGi.fx" application="org.eclipse.fx.ui.workbench.fx.application">
			<property name="appName" value="OSGi.fx" />
			<property name="applicationXMI" value="in.bytehue.osgifx.console.application/Application.e4xmi" />
			<property name="cssTheme" value="theme.default" />
			<property name="clearPersistedState" value="true" />
		</product>
	</extension>
</plugin>
